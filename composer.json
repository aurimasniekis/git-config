{
    "name": "aurimasniekis/git-config",
    "type": "library",
    "description": "Git Config value provider",
    "keywords": ["git-config", "git", "config"],
    "homepage": "https://github.com/aurimasniekis/git-config",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "php-mock/php-mock-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": { "AurimasNiekis\\GitConfig\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "AurimasNiekis\\GitConfig\\Test\\": "tests" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
